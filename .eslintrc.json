{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["unused-imports"],
  "rules": {
    "react/jsx-sort-props": [
      "error",
      {
        "callbacksLast": true,
        "shorthandLast": true
      }
    ],
    "import/order": "error",
    "unused-imports/no-unused-imports-ts": "error"
  }
}
